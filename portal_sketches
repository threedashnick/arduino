void setup() {
  pinMode(A5,INPUT_PULLUP);
  pinMode(12,INPUT);
  pinMode(13,OUTPUT);
}

void loop() {
  
  boolean button1 = !digitalRead(A5); //  ! - инвертирует сигнал полученный с кнопки
  
  if (buttom1 == 1) digitalWrite(13,1);
  else digitalWrite(13,0);

}   // включает 13ый пин ()встроенный светодиод при нажатии кнопки подключенной к пину А5 (кнопк gnd + А5)
